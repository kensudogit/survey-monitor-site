{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/login.html",
      "dest": "/public/login.html"
    },
    {
      "src": "/register.html",
      "dest": "/public/register.html"
    },
    {
      "src": "/surveys.html",
      "dest": "/public/surveys.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/public/dashboard.html"
    },
    {
      "src": "/survey.html",
      "dest": "/public/survey.html"
    },
    {
      "src": "/category.html",
      "dest": "/public/category.html"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "public": true,
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}
